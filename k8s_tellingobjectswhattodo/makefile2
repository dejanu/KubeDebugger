documentation:
	@echo "startapp"
    @echo "declarative_deployment"
    @echo "imperative_service"

startapp:
	@echo "docker run -p 8888:8888 dejanualex/go_hello:2.0"
declarative_deployment:
    @echo "kubectl create -f hello_deployment.yaml"
imperative_service:
    @echo "@echo "kubectl expose deploy webapp --name=webapp-svc --type="LoadBalancer" --port=8080 --target-port=8888""
